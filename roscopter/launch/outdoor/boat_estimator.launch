<?xml version="1.0"?>
<launch>
  <arg name="imu_topic" default="imu"/>
  <arg name="ublox_gnss" default="PosVelEcef"/>
  <arg name="ublox_relpos" default="compass/RelPos"/>
  <arg name="common_ref_lla" default="/ref_lla"/>
  
  <rosparam command="load" file="$(find roscopter)/params/target_ekf.yaml" ns="target_estimator"/>
  <node name="target_estimator" type="target_ekf_node" pkg="roscopter" output="screen">
    <remap from="imu" to="$(arg imu_topic)"/>
    <remap from="baro" to="dummy_topic"/>
    <!-- <remap from="reference" to="/truth/boat"/> -->
    <remap from="ublox_gnss" to="$(arg ublox_gnss)"/>
    <remap from="ublox_relpos" to="$(arg ublox_relpos)"/>
    <remap from="common_ref_lla" to="$(arg common_ref_lla)"/>
  </node>
</launch>
